{
  "name": "credit-card-rewards-api",
  "version": "1.0.0",
  "description": "A REST API that supports the credit-card-rewards front end",
  "author": "Brian Schneier",
  "main": "server.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/bschneier/credit-cards-api"
  },
  "engines": {
    "node": ">=6.9.2",
    "npm": ">=3.10.9"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.js ./test/**/*.js --color",
    "start": "node src/initializeApi",
    "start-dev": "SET NODE_ENV=dev& node src/initializeApi",
    "start-prod": "SET NODE_ENV=prod& node src/initializeApi",
    "prestart": "npm run lint && nsp check && npm test",
    "test": "nyc mocha test/**/*.test.js",
    "coverage": "nyc report"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "config": "^1.26.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.5",
    "express-mung": "^0.4.6",
    "express-request-id": "^1.4.0",
    "express-session": "^1.15.5",
    "jsonwebtoken": "^7.4.2",
    "mongoose": "^4.11.7",
    "redis": "^2.8.0",
    "secure-random-string": "^1.0.0",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "fakeredis": "^2.0.0",
    "mocha": "^3.5.0",
    "nsp": "^2.7.0",
    "nyc": "^11.1.0",
    "sinon": "^3.2.0",
    "sinon-chai": "^2.13.0"
  },
  "license": "MIT"
}
